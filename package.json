{
  "name": "polyglot-frontend",
  "version": "2.2.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --host polyglot --ssl true --ssl-key ~/.polyglot/ssl/polyglot_private.key --ssl-cert ~/.polyglot/ssl/polyglot.crt",
    "start:local": "ng serve --ssl true --ssl-key ~/.polyglot/ssl/polyglot_private.key --ssl-cert ~/.polyglot/ssl/polyglot.crt",
    "start:devhttp": "ng serve --host polyglot",
    "patchandrelease": "npm version patch --force",
    "postversion": "git push && git push --tags",
    "update:check": "ncu",
    "update:run": "ncu -u; npm install",
    "build:test": "ng build",
    "build:prod": "ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.8",
    "@angular/common": "8.2.8",
    "@angular/compiler": "8.2.8",
    "@angular/compiler-cli": "8.2.8",
    "@angular/core": "8.2.8",
    "@angular/forms": "8.2.8",
    "@angular/platform-browser": "8.2.8",
    "@angular/platform-browser-dynamic": "8.2.8",
    "@angular/platform-server": "8.2.8",
    "@angular/router": "8.2.8",
    "@angular/upgrade": "8.2.8",
    "@ng-bootstrap/ng-bootstrap": "5.1.1",
    "angular-in-memory-web-api": "0.9.0",
    "angular2-flash-messages": "^3.0.1",
    "buffer": "^5.4.3",
    "core-js": "3.2.1",
    "file-saver": "2.0.2",
    "mqtt": "3.0.0",
    "ngx-bootstrap": "5.1.2",
    "randomstring": "^1.1.5",
    "rxjs": "6.5.3",
    "setimmediate": "^1.0.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "8.3.6",
    "@types/node": "12.7.8",
    "lodash": "^4.17.15",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "3.5.3"
  }
}
